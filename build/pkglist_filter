#!/bin/sh

if which python2 2>/dev/null >/dev/null; then 
    exec python2 /home/antonio/Documentos/gsoc/cgal-public-dev/build/pkglist_filter.py "$1"
elif which python2.7 2>/dev/null >/dev/null; then 
    exec python2.7 /home/antonio/Documentos/gsoc/cgal-public-dev/build/pkglist_filter.py "$1"
elif which python2.6 2>/dev/null >/dev/null; then 
    exec python2.6 /home/antonio/Documentos/gsoc/cgal-public-dev/build/pkglist_filter.py "$1"
fi
